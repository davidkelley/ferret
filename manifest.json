{
  "manifest_version": 2,

  "name": "@@title",

  "version": "@@version",

  "description": "@@description",

  "homepage_url": "@@homepage",

  "offline_enabled": true,

  "minimum_chrome_version": "25",

  "app": {
    "background": {
      "scripts": [ "libs.js", "main.js" ]
    }
  },

  "permissions": ["usb", "fileSystem"],

  "optional_permissions": [ 
    @@devices
  ]

}
